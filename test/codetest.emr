    print("index = " + i) # string + number = concatination
    value = list[i]
    print("value = " + value)
    i = i + 1
    print("")
end for



# Fibonacci #
a = 1
b = 1
function fib()
    c = b
    b = a + b
    a = c
end function
for 20 do
    print(a)
    fib()
end for



